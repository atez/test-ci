{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2016",
    "outDir": "./dist",
    "moduleResolution": "node",
    "types": ["node","express"],
    "typeRoots": ["node_modules/@types"],
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "inlineSourceMap": true,
    "emitDecoratorMetadata": true
  },
  "include":[
    "./src/*.ts",
    "./src/*/*.ts"
  ],
  "exclude": []
}